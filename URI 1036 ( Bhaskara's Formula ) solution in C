#include<stdio.h>
int main()
{
    double a,b,c,p,r1,r2;
    scanf("%lf %lf %lf",&a,&b,&c);
    p=sqrt((b*b)-(4*a*c));
    r1=(-b+p)/(2*a);
    r2=(-b-p)/(2*a);
 
    if(((b*b)-4*a*c)<0 || a==0)
        printf("Impossivel calcular\n");
    else
        printf("R1 = %.5lf\nR2 = %.5lf\n",r1,r2);
    return 0;
}
