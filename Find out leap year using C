#include<stdio.h>
int main()
{
    int year;
    printf("Enter year : ");
    scanf("%d",&year);

    if(year%400==0)
        printf("%d is leap year",year); /*if a year is devided by 400
                                          then the year is leap year*/

    else if(year%4==0 && year%100!=0)
        printf("%d is leap year",year); /*if a year devided by 4 and not
                                          devided by 100 then this year
                                          is leap year*/
        else
        printf("%d is not leap year",year);

    return 0;
}
