 

#include <stdio.h>
int main()
{
    float i = 0;
    float j = 1;
    for(i=0; i<=2.2; i=i+0.2) //i must be less then 2.01/2.1/2.2/any large number but with in 2.2
    {
        for(j=i+1; j<=i+3.2; j=j+1) //j must be less then 3.01/3.1/3.2/any large number but with in 3.2
        {
            if(i==0 || i==1 || i>1.9)
                printf("I=%.0f J=%.0f\n",i,j);
            else
                printf("I=%.1f J=%.1f\n",i,j);
        }
    }
 
    return 0;
}
