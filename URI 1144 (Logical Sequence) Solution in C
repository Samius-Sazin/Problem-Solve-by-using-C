 

#include <stdio.h>
int main()
{
    int n,i,j,k;
    scanf("%d",&n);
 
    for(i=1; i<=n; i++)
    {
        for(j=i*i; j<=i*i; j++)
        {
            for(k=i*j; k<=i*j; k++)
            {
                printf("%d %d %d\n",i,j,k);
                printf("%d %d %d\n",i,j+1,k+1);
            }
        }
    }
    return 0;
}
