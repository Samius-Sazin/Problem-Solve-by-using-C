 

#include <stdio.h>
int main()
{
    int x,y,j,c=0,d=0,e=0,f=0;
    scanf("%d",&x);
    for(j=0; j<x; j++)
    {
        scanf("%d",&y);
        if(y%2==0) c++;
        if(y%3==0) d++;
        if(y%4==0) e++;
        if(y%5==0) f++;
    }
    printf("%d Multiplo(s) de 2\n",c);
    printf("%d Multiplo(s) de 3\n",d);
    printf("%d Multiplo(s) de 4\n",e);
    printf("%d Multiplo(s) de 5\n",f);;
 
    return 0;
}
